# API URL
VITE_API_URL=http://localhost:3000

# App Configuration
VITE_APP_NAME=SideBy
VITE_APP_VERSION=1.0.0
VITE_ENV=development

# Sentry (optional)
VITE_SENTRY_DSN=

# OAuth Configuration
VITE_GOOGLE_CLIENT_ID=

# Feature Flags
# Habilita login con Email/Password (por defecto solo Google OAuth)
VITE_FEATURE_EMAIL_LOGIN=false

# Habilita funcionalidad de análisis con IA en el wizard de datasets
VITE_FEATURE_AI_ENABLED=false

# Habilita edición de metadatos de datasets (botón Edit en DatasetCard)
VITE_FEATURE_DATASET_EDIT_ENABLED=true