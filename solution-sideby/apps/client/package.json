{
  "name": "sideby-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite preview --host 0.0.0.0",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest run src --exclude 'src/**/*integration*.test.{ts,tsx}'",
    "test:unit:watch": "vitest src --exclude 'src/**/*integration*.test.{ts,tsx}'",
    "test:integration": "vitest run src 'src/**/*integration*.test.{ts,tsx}'",
    "test:integration:watch": "vitest src 'src/**/*integration*.test.{ts,tsx}'",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@hookform/resolvers": "^5.2.2",
