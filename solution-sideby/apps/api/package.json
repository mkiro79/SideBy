{
  "name": "sideby-api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file .env src/index.ts",
    "dev:docker": "tsx watch src/index.ts",
    "build": "tsc && node scripts/rewrite-dist-aliases.mjs",
    "start": "node dist/index.js",
    "start:local": "node --env-file .env dist/index.js",
    "seed": "tsx src/scripts/seed.ts",
    "lint": "eslint .",
    "test": "vitest",
    "test:run": "vitest run --exclude=**/*repository.test.ts --exclude=**/*.integration.test.ts",
    "test:unit": "vitest run --exclude=**/*repository.test.ts --exclude=**/*.integration.test.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.4.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.9.5",
    "multer": "^2.0.2",
    "papaparse": "^5.5.3",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.6",
    "@types/papaparse": "^5.5.2",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.17.0",
    "mongodb-memory-server": "^11.0.1",
    "supertest": "^7.2.2",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vitest": "^4.0.18"
  }
}
